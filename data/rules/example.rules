# Example Suricata Rules for Testing
# These are sample rules to demonstrate the browser functionality

alert tcp any any -> any 80 (msg:"ET WEB_SERVER Possible SQL Injection Attempt SELECT FROM"; flow:to_server,established; content:"SELECT"; nocase; content:"FROM"; nocase; distance:0; classtype:web-application-attack; sid:2000001; rev:1; metadata:created_at 2024_01_01, updated_at 2024_01_01;)

alert tcp any any -> any 443 (msg:"ET POLICY OpenSSL Demo CA"; flow:to_client,established; content:"|55 04 03|"; content:"OpenSSL"; distance:1; within:7; classtype:policy-violation; sid:2000002; rev:2; metadata:created_at 2024_01_01, updated_at 2024_01_15;)

drop tcp any any -> $HOME_NET 22 (msg:"ET EXPLOIT SSH Brute Force Attempt"; flow:to_server,established; content:"SSH-"; depth:4; threshold:type threshold, track by_src, count 5, seconds 60; classtype:attempted-admin; priority:1; sid:2000003; rev:1; reference:url,docs.example.com/ssh-attacks;)

alert http any any -> any any (msg:"ET WEB_SERVER PHP Remote File Inclusion Attempt"; flow:to_server,established; content:"GET"; http_method; content:".php"; http_uri; content:"http://"; http_uri; classtype:web-application-attack; priority:1; sid:2000004; rev:3;)

alert tcp any any -> any 3389 (msg:"ET EXPLOIT Microsoft RDP CVE-2019-0708 BlueKeep Exploit Attempt"; flow:to_server,established; content:"|03 00|"; depth:2; classtype:attempted-admin; priority:1; sid:2000005; rev:1; reference:cve,2019-0708; reference:url,example.com/bluekeep;)

alert dns any any -> any any (msg:"ET DNS Query to Suspicious TLD .tk"; dns_query; content:".tk"; nocase; classtype:policy-violation; priority:2; sid:2000006; rev:1;)

alert tls any any -> any any (msg:"ET MALWARE Possible TLS Certificate Subject CN=Malware"; tls_cert_subject; content:"CN=Malware"; classtype:trojan-activity; priority:1; sid:2000007; rev:1;)

alert http any any -> any any (msg:"ET WEB_SERVER Suspicious User-Agent sqlmap"; flow:to_server,established; content:"User-Agent|3a 20|sqlmap"; http_header; classtype:web-application-attack; priority:1; sid:2000008; rev:2;)

alert tcp any any -> any 445 (msg:"ET EXPLOIT Windows SMB Remote Code Execution Attempt"; flow:to_server,established; content:"|ff|SMB"; depth:5; classtype:attempted-admin; priority:1; sid:2000009; rev:1; reference:url,example.com/smb-exploits;)

alert icmp any any -> $HOME_NET any (msg:"ET SCAN ICMP Ping Sweep"; itype:8; threshold:type threshold, track by_src, count 10, seconds 10; classtype:attempted-recon; priority:3; sid:2000010; rev:1;)

alert http any any -> any any (msg:"ET WEB_CLIENT Possible XSS Attempt in URI"; flow:to_server,established; content:"<script"; http_uri; nocase; classtype:web-application-attack; priority:2; sid:2000011; rev:1;)

alert tcp any any -> any 21 (msg:"ET FTP Anonymous Login Attempt"; flow:to_server,established; content:"USER anonymous"; nocase; classtype:attempted-admin; priority:2; sid:2000012; rev:1;)

pass tcp any any -> any 80 (msg:"ET POLICY HTTP to External Network"; flow:to_server,established; classtype:policy-violation; priority:3; sid:2000013; rev:1;)

alert udp any any -> any 53 (msg:"ET DNS Query for Known Malware Domain"; flow:to_server; content:"|01 00 00 01|"; offset:2; depth:4; content:"malware"; nocase; classtype:trojan-activity; priority:1; sid:2000014; rev:2; reference:url,example.com/malware-domains;)

alert tcp any any -> any any (msg:"ET EXPLOIT Buffer Overflow Attempt Excessive NOP Sled"; flow:to_server,established; content:"|90 90 90 90 90 90 90 90 90 90|"; classtype:shellcode-detect; priority:1; sid:2000015; rev:1;)
